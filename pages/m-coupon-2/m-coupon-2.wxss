@import "../../lib/style/iconfont.wxss";
image{
  width: 100%;
  display: flex;
}
view{
  margin: 0;
  padding: 0;
}
.position{
  position: relative;
}
.position view{
  position: absolute;
  width: 468rpx;
  height: 90rpx;
  background:url('https://m.7710mall.com/Uploads/templet/5.png') no-repeat center;
  background-size:468rpx 90rpx; 
  left: 142rpx;
  bottom: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

/* 设置背景遮罩层样式 */
.mask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
/* 设置遮罩内容样式 */
.maskContent {
  width: 70%;
  background: #fff;
  border-radius: 5px;
  padding: 30rpx;
}
