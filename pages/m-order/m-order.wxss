@import "../../lib/style/iconfont.wxss";
@import "../../lib/style/public.wxss";
/* 切换 */
.nav {
  height: 88rpx;
  background: #fff;
}
.nav .nav-box{
  width: 100%;
  height:87rpx;
  overflow: hidden;
}
.nav .nav-box view {
  float: left;
  font-size: 30rpx;
  height: 87rpx;
  line-height: 87rpx;
  width: 20%;
  text-align: center;
  color: #303030;
  box-sizing: border-box;
}
.nav .nav-box .active {
  color: #ff712b;
  border-bottom: 2rpx solid #ff712b;
}
/* 空 */
kong {
  width: 100%;
  overflow: hidden;
}
.kong view {
  text-align: center;
}
.kong view .iconfont {
  font-size: 190rpx;
  color: #cacaca;
}
.kong view .tt {
  font-size:28rpx;
  color: #cacaca;
}
/* 订单 */
.order-info {
  background: #fff;
  margin-top: 10rpx;
  padding: 0 32rpx;
}
.order-info .order-Num {
  height: 80rpx;
}
.order-info .order-Num .order-bh {
  font-size: 26rpx;
  font-weight: normal;
  color: #303030;
  line-height: 80rpx;
}
.order-info .order-Num .order-state{
  font-size:26rpx;
  color: #ff712b;
  line-height: 80rpx;
  float: right;
}
.order-info .good {
  padding-top: 25rpx;
}
.order-info .big-box {
  padding-top: 25rpx;
}
.order-info .big-box image {
  width: 160rpx;
  height: 160rpx;
  float: left;
  margin: 5rpx 5.75rpx;
}
.order-info .good .left {
  float: left;
  width: 160rpx;
  height: 160rpx;
}
.order-info .good .left image {
  width: 100%;
  height: 100%;
}
.order-info .good .right {
  float: left;
  margin-left: 25rpx;
  width: 500rpx;
  height: 160rpx;
  position: relative;
}
.order-info .good .right .goods-name {
  font-size:32rpx;
  color: #303030;
  line-height: 55rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  width:500rpx;
  display:block;
}
.order-info .good .right .goods-feature {
  font-size: 28rpx;
  color: #666;
  margin-top: 10rpx;
  line-height: 35rpx;
  width: 400rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.order-info .good .right .goods-price {
  font-size:32rpx;
  color: #FF712B;
  position: absolute;
  bottom: 0;
}
.order-info .good .right .number {
  float: right;
  font-size: 26rpx;
  line-height: 55rpx;
  color: #666;
}
.order-info .order-money {
  text-align: right;
  line-height: 70rpx;
}
.order-info .order-money text {
  line-height:70rpx;
}
.order-info .operation {
  height: 110rpx;
  text-align: right;
}
.order-info .operation text {
  display: inline-block;
  width: 140rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
  border-radius: 10rpx;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  -ms-border-radius: 10rpx;
  -o-border-radius: 10rpx;
  margin-top: 28rpx;
}
.order-info .operation .service {
  float: left;
  font-size:24rpx;
  color: #f00;
  line-height: 110rpx;
}
.order-info .operation navigator{
  display: inline-block;
}
.order-info .operation .cancel,
.order-info .operation .logistics,
.order-info .operation .delete,
.order-info .operation .link-p,
.order-info .operation .buy {
  border: 1rpx solid #bababa;
  color: #303030;
}
.order-info .operation .Nchange {
  color: #666;
  border: 1rpx solid #c9c9c9;
}
.order-info .operation .pay,
.order-info .operation .receive,
.order-info .operation .quik,
.order-info .operation .estimate,
.order-info .operation .change,
.order-info .operation .refunding,
.order-info .operation .detail {
  border: 1rpx solid #ff712b;
  color: #ff712b;
}
.order-info .operation .cant {
  border: 1rpx solid #c9c9c9;
  color: #666;
}

/* 优惠券 */
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background: #000;
  opacity: 0.5;
  z-index: 999;
  /* display: none; */
}
.success{
  width: 690rpx;
  height: 900rpx;
  position: fixed;
  top:50%;
  left: 50%;
  margin-top: -450rpx;
  margin-left: -345rpx;
  z-index: 1000;
  /* display: none; */
}
.success .close{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top:-50rpx;
  right: 40rpx;
}
.success .bg{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.success .success-con{
  position: absolute;
  top: 130rpx;
  left: 0;
  text-align: center;
  width: 100%;
  z-index: 1001;
}
.success .success-con .success-img{
  width: 186rpx;
  height: 186rpx;
  margin-bottom: 5rpx;
}
.success .success-con .one{
  font-size: 22rpx;
  color: #913407;
  line-height: 36rpx;
}
.success .success-con .two{
  font-size: 18rpx;
  color: #666;
}
.success  .op-box{
  width: 100%;
  position: absolute;
  bottom: 160rpx;
  left: 0;
  z-index: 1002;
  text-align: center;
}
.success  .use{
  margin: 0 auto;
  margin-top: 50rpx;
  width: 504rpx;
  height: 86rpx;
  line-height: 86rpx;
  text-align: center;
  background: #fff;
  color: #ff712b;
  font-size: 32rpx;
  border-radius: 50rpx;
  -o-border-radius:50rpx; 
  -ms-border-radius:50rpx; 
  -moz-border-radius:50rpx; 
  -webkit-border-radius:50rpx; 
}
.success  .again{
  margin: 0 auto;
  margin-top: 20rpx;
  width: 504rpx;
  height: 86rpx;
  line-height: 86rpx;
  text-align: center;
  border: 1rpx solid #fff;
  color: #fff;
  font-size: 32rpx;
  border-radius: 50rpx;
  -o-border-radius:50rpx; 
  -ms-border-radius:50rpx; 
  -moz-border-radius:50rpx; 
  -webkit-border-radius:50rpx; 
}

/* share */
.share{
  position: fixed;
  top:0;
  left:0;
  bottom: 0;
  right:0;
  z-index: 10;
  background-color: rgba(0,0,0,.5);
  display: flex;
}
.share image{
  width: 416rpx;
  height: 691rpx;
  display: block;
  position: absolute;
  top:50%;
  margin-top: -346rpx;
  left:50%;
  margin-left: -208rpx;
  z-index: 11;
}
.shareBox{
  position: relative;
  z-index: 12;
  width: 100%;
  height: 100%;
}
.shareBox button{
  position:absolute;
  left:50%;
  height: 80rpx;
  width: 350rpx;
  margin-left:-180rpx;
  z-index: 13;
  top: 58%;
  background:rgba(0,0,0,0);
  border:none;
}
button::after{
  border: none;
}
.share .shareClose{
  position:absolute;
  left:50%;
  width:80rpx;
  height:80rpx;
  margin-left:-40rpx;
  top:73%;
  /* background-color: #fff */
}