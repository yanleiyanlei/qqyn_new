/* pages/m-withdraw-d/m-withdraw-d.wxss */

/* 切换 */
@import "../../lib/style/iconfont.wxss";
@import "../../lib/style/public.wxss";

.nav {
  height: 88rpx;
  background: #fff;
}

.nav .nav-box {
  width: 100%;
  height: 87rpx;
  overflow: hidden;
}

.nav .nav-box view {
  font-size: 30rpx;
  height: 87rpx;
  line-height: 87rpx;
  width: 20%;
  text-align: center;
  color: #303030;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
}

.nav .nav-box .active {
  color: #ff712b;
  border-bottom: 2rpx solid #ff712b;
}

/* //// */

.box {
  padding: 0 33rpx;
  background: #fff;
  margin-top: 20rpx;
}

.list {
  width: 100%;
  /* background: #fff; */
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  /* border-bottom: 1rpx solid #d9d6d6; */
}

.list .pay {
  font-weight: normal;
  font-size: 28rpx;
  line-height: 43rpx;
  color: #303030;
}

.list .pay .span {
  float: right;
}

.list .pay .i {
  float: right;
  color: #aaa;
  margin-right: 10rpx;
}

.list .pay .b {
  float: right;
  color: #aaa;
  font-weight: 100;
}
