/* pages/m-withdraw-d/m-withdraw-d.wxss */

/* 切换 */
@import "../../lib/style/iconfont.wxss";
@import "../../lib/style/public.wxss";

.nav {
  height: 86rpx;
  background: #fff;
}

.nav .nav-box {
  width: 100%;
  height: 100%;
  overflow: hidden;
  display: flex;
  justify-content: space-around;
}

.nav .nav-box view {
  font-size: 30rpx;
  height: 87rpx;
  line-height: 87rpx;
  width: 100rpx;
  text-align: center;
  color: #303030;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
}

.nav .nav-box .active {
  color: #ff712b;
  border-bottom: 4rpx solid #ff712b;
}

/* //// */

.all {
  width: 100%;
  height: 68rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 30rpx;
  color: #333;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.all view:first-child{
  margin-right: 20rpx;
}
.all text {
  color: #ff6c00;
}

.box {
  width: 100%;
}

.box-li {
  width: 100%;
  height: 260rpx;
  background: #fff;
  margin-bottom: 10rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.box-li-top {
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid #e5e5e5;
  padding-bottom: 16rpx;
  font-size: 28rpx;
  color: #333;
}

.box-li-bottom {
  padding-top: 36rpx;
  font-size: 30rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
}

.box-li-bottom .box-jin {
  color: #ff6c00;
}

.box-li-top .box-success {
  color: #00b30d;
}

.box-li-top .box-exam {
  color: #fe3f3f;
}

.box-li-top .box-make {
  color: #4095ff;
}

.disright {
  display: flex;
  justify-content: flex-end;
}
