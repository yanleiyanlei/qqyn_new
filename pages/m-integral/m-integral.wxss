/*200 积分 */
.mypoints .back-mypo .jf {
  width: 142rpx;
  height: 65rpx;
  background: none;
  font-size: 40rpx;
  color: #ff712b;
  position: absolute;
  left: 310rpx;
  top: 180rpx;
  z-index: 22;
  font-weight: bold;
  line-height: 65rpx;
  text-align: center;
}

.mypoints .back-mypo .jf text {
  font-size: 24rpx;
  font-weight: normal;
}
/* 积分明细标题 */
.mypoints .logbook {
  width: 100%;
  height: 100rpx;
  background: none;
  padding-left: 30rpx;
  box-sizing: border-box;
  line-height: 100rpx;
  display: -webkit-box;
  -webkit-box-orient: horizontal;
}

.mypoints .logbook text {
  display: block;
  font-size: 30rpx;
  color: #303030;
  margin-left: 10rpx;
  line-height: 100rpx;
}

.mypoints .logbook image {
  width: 45rpx;
  height: 45rpx;
  background: none;
  margin-top: 25rpx;
  

}
/* 积分列表 */
.mypoints .poinstiafg {
  width: 100%;
  height: auto;
  background: #fff;
  padding-left: 30rpx;
  box-sizing: border-box;
}

.mypoints .poinstiafg .dl {
  width: 100%;
  height: 130rpx;
  background: #fff;
  position: relative;
   border-bottom: 1rpx solid #bfbfbf;
}

.mypoints .poinstiafg .dl .dt {
  color: #303030;
  font-size: 28rpx;
  padding-top: 30rpx;
}

.mypoints .poinstiafg .dl .dd {
  font-size: 22rpx;
  color: #909090;
  padding-top: 15rpx;
}

.mypoints .poinstiafg .dl .dd text {
  margin-left: 15rpx;
}

.mypoints .poinstiafg .dl .p {
  color: #303030;
  font-size: 28rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}


