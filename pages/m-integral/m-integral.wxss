.mypoints {
  width: 100%;
  height: 100%;

  display: -webkit-box;
  -webkit-box-orient: vertical;
  position: relative;
}

.mypoints .back-mypo {
  width: 100%;
  height: 300rpx;
  background: none;
}

.mypoints .back-mypo image {
  width: 100%;
  height: 100%;
  background: none;
}

.mypoints .back-mypo .image-rba image {
  width: 246rpx;
  height: 246rpx;
  background: none;
  position: absolute;
  top: 80rpx;
  left: 250rpx;
}
/* 任务积分 */
.mypoints .back-mypo .positione {
  position: absolute;
  top: 120rpx;
  left: 110rpx;
  text-align: center;
  height:92rpx;
  /* background:red; */
}

.mypoints .back-mypo .positione .dt .log-rw {
  width: 57rpx;
  height: 57rpx;
  background: none;
}

.mypoints .back-mypo .positione .dd {
  font-size: 28rpx;
  color: #fff;
}
/* 签到 */
