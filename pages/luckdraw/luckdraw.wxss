/* apges/luckdraw/luckdraw.wxss */
@import "../../lib/style/public.wxss";
.container-bg { background-repeat:no-repeat;
padding-top: 40rpx;
  background-image:url(data:image/jpeg;base64,);}
.container-out {
  height: 600rpx;
  width: 650rpx;
  margin: 0 auto;
  background-color: #FFD905;
  border-radius: 40rpx;
  position: relative;
}
.have-times{
  position: relative;
  top:-30px;
  left:25%;
  color:#f5f0fc;
}
.container-in {
  width: 580rpx;
  height: 530rpx;
  background-color: #F49805;
  border-radius: 40rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}

/**小圆球
box-shadow: inset 3px 3px 3px #fff2af;*/

.circle {
  position: absolute;
  display: block;
  border-radius: 50%;
  height: 20rpx;
  width: 20rpx;
}

.content-out {
  position: absolute;
  height: 150rpx;
  width: 166.6666rpx;
  background-color: #f5f0fc;
  border-radius: 15rpx;
  box-shadow: 0 5px 0 #e7930a;
}

/**居中 加粗*/

.start-btn {
  position: absolute;
  margin: auto;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  border-radius: 15rpx;
  height: 150rpx;
  width: 166.6666rpx;
  background-color: #ffe400;
  box-shadow: 0 5px 0 #e7930a;
  color: #f6251e;
  text-align: center;
  font-size: 35rpx;
  font-weight: bolder;
  line-height: 150rpx;
}

.award-image {
  position: absolute;
  margin: auto;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  height: 120rpx;
  width: 140rpx;
}
.page_bd{
  position: relative;
}
.gohome{
  position: absolute;
  bottom: 100rpx;
  display: flex;
  justify-content: center;
  color: #e9bd64;
  left: 50%;
  transform: translateX(-50%);
}