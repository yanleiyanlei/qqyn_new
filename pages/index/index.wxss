/*搜索===========  */

.search {
  position: absolute;
  top: 0;
  z-index: 9999;
  display: flex;
  width: 100%;
  box-sizing: border-box;
  direction: center;
  font-size: 24rpx;
  color: #fff;
  padding: 20rpx;
  flex-direction: row;
  justify-content: space-between;
}

.search .searchLoc {
  height: 60rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.search .searchLoc image {
  width: 27rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.search .searchLoc text {
  font-size: 30rpx;
  font-family: SourceHanSansCN-Medium;
  font-weight: 500;
  color: rgba(248, 248, 248, 1);
  width: 100rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.search .searchCont {
  width: 550rpx;
  height: 60rpx;
  box-sizing: border-box;
  background: rgba(248, 248, 248, 0.95);
  box-shadow: 0px 0px 38rpx 0rpx rgba(190, 190, 195, 0.15);
  border-radius: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-left: 24rpx;
  font-size: 28rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: 400;
  color: rgba(149, 149, 149, 1);
}

.search .searchCont text {
  font-size: 28rpx;
  color: rgba(149, 149, 149, 1);
  margin-right: 34rpx;
}

/*swiper======  */

.index-swiper {
  position: relative;
}

.dots {
  width: 100%;
  position: absolute;
  bottom: 14rpx;
  display: flex;
  justify-content: center;
}

.dots .dot {
  transition: all 0.2;
  height: 6rpx;
  background: #fff;
}

.dots .dotactive {
  width: 28rpx;
}

.dots .nodotactive {
  width: 12rpx;
  margin: 0 8rpx;
  border-radius: 3rpx;
}

.dots .dotactive:first-child {
  border-bottom-left-radius: 3rpx;
  border-top-left-radius: 3rpx;
}

.dots .dotactive:last-child {
  border-bottom-right-radius: 3rpx;
  border-top-right-radius: 3rpx;
}

.index-swiper swiper {
  height: 370rpx;
}

.index-swiper image {
  width: 100%;
  height: 370rpx;
}

/*hint========  */

.hint {
  background: #fff;
  display: flex;
  justify-content: space-around;
  font-size: 24rpx;
  font-family: SourceHanSansCN-Light;
  font-weight: 300;
  color: rgba(85, 85, 85, 1);
  /* padding-top: 14rpx; */
}

.hint view {
  display: flex;
  height: 104rpx;
  align-items: center;
}

.hint view:first-child image {
  width: 22rpx;
  height: 25rpx;
  margin-right: 8rpx;
}

.hint view:nth-child(2) image {
  width: 24rpx;
  height: 23rpx;
  margin-right: 8rpx;
}

.hint view:last-child image {
  width: 22rpx;
  height: 23rpx;
  margin-right: 8rpx;
}

