@import "../../lib/style/iconfont.wxss";

page {
  width: 100%;
  height: 100%;
  background: #eee;
}

navigator .navigator-hover {
  opacity: 1;
}

.shopcartitle {
  width: 100%;
  height: 100rpx;
  background: #fff;
  line-height: 100rpx;
  color: #434343;
  position: fixed;
  top: 0;
  z-index: 999999999;
  left: 0;
  border-bottom: 1rpx solid #e2e2e2;
}

.shopcartitle .sh-title {
  text-align: center;
  font-size: 34rpx;
  font-weight: 480;
  display: none;
}

.shopcartitle .bianji {
  position: absolute;
  top: 3rpx;
  right: 32rpx;
  display: block;
  font-weight: 500;
  font-size: 30rpx;
  font-family: SourceHanSansCN-Medium;
  color: rgba(53, 53, 53, 1);
}

.cart-box {
  width: 100%;
  height: 237rpx;
  border-bottom: 1rpx solid #e2e2e2;
  display: flex;
  background: #fff;
  flex-direction: row;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  justify-content: space-between;
  position: relative;
  overflow: hidden;
}

.cartboxs {
  width: 100%;
  overflow: hidden;
}

.content {
  height: 100%;
  margin-right: 0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(127rpx);
  transform: translateX(127rpx);
  margin-left: -127rpx;
}

.del {
  position: absolute;
  right: 0;
  top: 0;
  background:rgba(225,55,59,1);
  width: 127rpx;
  height: 237rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 26rpx;
  -webkit-transform: translateX(127rpx);
  transform: translateX(127rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .content, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.cart-box icon {
  margin-top: 90rpx;
  display: block;
  float: left;
  font-size: 40rpx;
}

.imgtex {
