/** 文字滚动 */
.box{
  position: relative;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  overflow: hidden;
  margin-top: 100rpx
}

.txt{
  white-space: nowrap;
}

#txt1{
  position: relative;
  white-space: nowrap;
  transition: left 1s linear;
  font-size:22rpx;
  font-family:SourceHanSansCN-Regular;
  font-weight:400;
  color:rgba(130,130,130,1);
  line-height:30px
}

#txt2,.flex-box{
  position: absolute;
  top: 0;
}

.flex-box{
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
}

@keyframes roll {
  0% {left: 750rpx;}
  100% {left: -100%;}
}